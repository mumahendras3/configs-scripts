#!/bin/execlineb -P

##################################################
# Start pulseaudio (The PulseAudio Sound System) #
##################################################

fdmove -c 2 1
if { ln -s ../../messagebus-srv/env/DBUS_SESSION_BUS_ADDRESS env }
s6-envdir env
multisubstitute {
  importas -u CMD CMD
  importas -usCd" " CMD_OPTS CMD_OPTS
}
emptyenv -c $CMD $CMD_OPTS
