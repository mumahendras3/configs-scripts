#!/bin/execlineb -P

###################################
# Start the pipewire-pulse daemon #
###################################

fdmove -c 2 1
if { ln -s ../../dbus-session-bus/env/DBUS_SESSION_BUS_ADDRESS env }
s6-envdir env
multisubstitute {
  importas -u CMD CMD
  importas -usCd" " CMD_OPTS CMD_OPTS
}
emptyenv -c $CMD $CMD_OPTS
